{
  "name": "expedientes-ike-bot",
  "version": "1.0.0",
  "description": "Bot de Telegram para automatización de validación de expedientes IKE",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "build:simple": "tsc -p tsconfig.simple.json",
    "start": "node dist/main.js",
    "dev": "nodemon --exec ts-node src/main.ts",
    "dev:watch": "ts-node --watch src/main.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run format:check && npm run lint && npm run type-check",
    "prepare": "husky install || true"
  },
  "keywords": [
    "telegram",
    "bot",
    "expedientes",
    "ike",
    "automation",
    "excel"
  ],
  "author": "IKE Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "puppeteer": "^21.7.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "telegraf": "^4.15.0",
    "telegraf-session-local": "^2.1.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
